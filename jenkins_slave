pipeline {
    agent any
    parameters {
        string(name: 'NAME', defaultValue: '', description: 'Enter your name')
        string(name: 'Ages', defaultValue: '', description: 'Enter your age')
    }
    stages {
        stage('Example') {
            steps {
                echo "Hello from Slave ${params.NAME} of ${params.Ages}"
                sh 'python3 myscript.py ${params.NAME} --age ${params.Ages}'
                echo "Successful"
            }
        }
    }
}